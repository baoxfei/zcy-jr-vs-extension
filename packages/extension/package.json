{
  "name": "zcy-jr-snippet-manager",
  "displayName": "ZcyJrSnippetManager",
  "description": "",
  "version": "0.0.1",
  "publisher": "baoxfei",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:zcy-jr-snippet-manager",
    "onView:zcy-jr-snippet-manager.personalSnippets",
    "onView:zcy-jr-snippet-manager.publicSnippets"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "zcy-jr-snippet-manager.openSetting",
        "title": "设置",
        "icon": "$(gear)"
      },
      {
        "command": "zcy-jr-snippet-manager.showGenerateSnippet",
        "title": "打开生成代码片段页"
      },
      {
        "command": "zcy-jr-snippet-manager.generateSnippet",
        "title": "生成代码片段View"
      },
      {
        "command": "zcy-jr-snippet-manager.closeGenerateSnippet",
        "title": "关闭代码片段页"
      },
      {
        "command": "zcy-jr-snippet-manager.backToWelcome",
        "title": "返回上一页",
        "icon": "$(arrow-left)"
      },
      {
        "command": "zcy-jr-snippet-manager.refreshPersonalTreeView",
        "title": "刷新代码片段树视图",
        "icon": "$(refresh)"
      },
      {
        "command": "zcy-jr-snippet-manager.exportPersonalSnippet",
        "title": "导出代码片段",
        "icon": "$(cloud-download)"
      },
      {
        "command": "zcy-jr-snippet-manager.deletePersonalSnippet",
        "title": "删除代码片段",
        "icon": "$(trash)"
      },
      {
        "command": "zcy-jr-snippet-manager.editPersonalSnippet",
        "title": "编辑代码片段",
        "icon": "$(edit)"
      },
      {
        "command": "zcy-jr-snippet-manager.importPersonalSnippet",
        "title": "导入代码片段",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "zcy-jr-snippet-manager.searchPersonalSnippet",
        "title": "搜索代码片段",
        "icon": "$(search)"
      },
      {
        "command": "zcy-jr-snippet-manager.refreshPublicSnippets",
        "title": "刷新代码片段树视图",
        "icon": "$(refresh)"
      },
      {
        "command": "zcy-jr-snippet-manager.searchPublicSnippet",
        "title": "搜索代码片段",
        "icon": "$(search)"
      },
      {
        "command": "zcy-jr-snippet-manager.deletePublicSnippet",
        "title": "删除代码片段",
        "icon": "$(trash)"
      },
      {
        "command": "zcy-jr-snippet-manager.uploadPublicSnippet",
        "title": "同步代码片段",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "zcy-jr-snippet-manager.exportPublicSnippet",
        "title": "导出代码片段",
        "icon": "$(cloud-download)"
      },
      {
        "command": "zcy-jr-snippet-manager.editPublicSnippet",
        "title": "编辑代码片段",
        "icon": "$(edit)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zcy-jr-snippet-manager",
          "title": "ZcyJr",
          "icon": "images/snippets.svg"
        }
      ]
    },
    "views": {
      "zcy-jr-snippet-manager": [
        {
          "id": "zcy-jr-snippet-manager.generateSnippet",
          "name": "welcome",
          "when": "!zcy-jr-snippet-manager.snippetView",
          "visibility": "hidden"
        },
        {
          "id": "zcy-jr-snippet-manager.snippetView",
          "name": "snippetView",
          "type": "webview",
          "when": "zcy-jr-snippet-manager.snippetView"
        },
        {
          "id": "zcy-jr-snippet-manager.personalSnippets",
          "name": "personalSnippets",
          "type": "tree",
          "when": "!zcy-jr-snippet-manager.snippetView"
        },
        {
          "id": "zcy-jr-snippet-manager.publicSnippets",
          "name": "publicSnippets",
          "type": "tree",
          "when": "!zcy-jr-snippet-manager.snippetView"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "zcy-jr-snippet-manager.generateSnippet",
        "contents": "[设置](command:zcy-jr-snippet-manager.openSetting)\n[代码片段生成](command:zcy-jr-snippet-manager.showGenerateSnippet)"
      }
    ],
    "configuration": {
      "title": "ZcyJr",
      "properties": {
        "ZcyJr.publicSnippetsConfig": {
          "type": "object",
          "description": "公共片段仓库配置",
          "properties": {
            "gitRepoUrl": {
              "type": "string",
              "default": "",
              "description": "公共代码片段地址"
            },
            "authorizationToken": {
              "type": "string",
              "default": "",
              "description": "公共代码片段仓库token"
            },
            "gistId": {
              "type": "string",
              "default": "",
              "description": "公共仓库代码片段id"
            },
            "gistFileName": {
              "type": "string",
              "default": "public.code-snippets",
              "description": "公共代码片段文件名"
            }
          }
        }
      }
    },
    "menus": {
      "view/title": [
        {
          "command": "zcy-jr-snippet-manager.backToWelcome",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.snippetView"
        },
        {
          "command": "zcy-jr-snippet-manager.refreshPersonalTreeView",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.personalSnippets"
        },
        {
          "command": "zcy-jr-snippet-manager.importPersonalSnippet",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.personalSnippets"
        },
        {
          "command": "zcy-jr-snippet-manager.searchPersonalSnippet",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.personalSnippets"
        },
        {
          "command": "zcy-jr-snippet-manager.refreshPublicSnippets",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.publicSnippets"
        },
        {
          "command": "zcy-jr-snippet-manager.uploadPublicSnippet",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.publicSnippets"
        },
        {
          "command": "zcy-jr-snippet-manager.searchPublicSnippet",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.publicSnippets"
        },
        {
          "command": "zcy-jr-snippet-manager.exportPublicSnippet",
          "group": "navigation",
          "when": "view === zcy-jr-snippet-manager.publicSnippets"
        }
      ],
      "view/item/context": [
        {
          "command": "zcy-jr-snippet-manager.deletePersonalSnippet",
          "group": "inline",
          "when": "view === zcy-jr-snippet-manager.personalSnippets && viewItem === personalSnippetItem"
        },
        {
          "command": "zcy-jr-snippet-manager.editPersonalSnippet",
          "group": "inline",
          "when": "view === zcy-jr-snippet-manager.personalSnippets && viewItem === personalSnippetItem"
        },
        {
          "command": "zcy-jr-snippet-manager.deletePublicSnippet",
          "group": "inline",
          "when": "view === zcy-jr-snippet-manager.publicSnippets && viewItem === publicSnippetItem"
        },
        {
          "command": "zcy-jr-snippet-manager.editPublicSnippet",
          "group": "inline",
          "when": "view === zcy-jr-snippet-manager.publicSnippets && viewItem === publicSnippetItem"
        }
      ]
    },
    "snippets": [
      {
        "path": "./snippets/personal.code-snippets"
      },
      {
        "path": "./snippets/public.code-snippets"
      }
    ]
  },
  "scripts": {
    "watch": "cross-env NODE_ENV=development webpack --watch",
    "build:webview": "yarn --cwd \"webview-view\"  build",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "clean:cache": "rm -rf node_modules/.cache && webpack --clear-cache",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.17",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "copy-webpack-plugin": "^13.0.0",
    "eslint": "^9.19.0",
    "htmlparser2": "^10.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@octokit/core": "^6.1.5",
    "axios": "^1.9.0",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.0",
    "lodash": "^4.17.21"
  }
}
